{
	"name": "plopit",
	"version": "0.0.1",
	"description": "A small set of templates for plop with a focus on react development",
	"repository": "https://github.com/keyzog/plopit.git",
	"keywords": [
		"plop",
		"react templates",
		"react generator"
	],
	"type": "module",
	"module": "./dist/index.js",
	"main": "./dist/index.cjs",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"bin": {
		"plopit": "./dist/bin/global.js"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@types/minimist": "^1.2.5",
		"@types/node": "^20.14.9",
		"typescript": "^5.2.2",
		"vite": "^5.3.1",
		"vite-plugin-commonjs": "^0.10.1",
		"vite-plugin-node-externals": "^0.0.1",
		"vite-plugin-static-copy": "^1.0.5"
	},
	"dependencies": {
		"minimist": "^1.2.8",
		"plop": "^4.0.1"
	}
}
